#include <ISEN.h>
#include <LabIsen-2009.h>
#include <ISL12026.h>
#include <MCP23X08.h>
#include <LiquidCrystal.h>

void setup() {
  ISENLiquidCrystal lcd;
  lcd.begin(16,2);
  lcd.clear();
  lcd.setCursor(0,0);
  lcd.print("HELLO");
  lcd.setCursor(0,1);
  lcd.print("WORLD!");
  
}
void loop() {
}
