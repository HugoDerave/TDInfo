#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>

int main()
{ 
	int annee;
	printf("rentrez une annee\n");
	scanf_s("%d", &annee);
	if (annee % 4 == 0 && annee %100!=0 || ((annee % 100 == 0) && (annee % 400 == 0)))
	{
		printf("%d est une annee bissextile\n", annee);
	}
	else
	{
		printf("%d n'est pas une annee bissextile\n", annee);
	}
	system("pause");
	return (EXIT_SUCCESS);
}
