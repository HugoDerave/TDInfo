#include <stdlib.h>
#include <stdio.h>
#include <string.h>
void swap(int *A, int *B)
{
	int tmp;
	tmp = *A;
	*A = *B;
	*B = tmp;
}
void ordonne(int *A, int *B)
{
	int c;
	int d;
	c = *A;
	d = *B;
	if (c < d)
	{
		int tmp;
		tmp = *A;
		*A = *B;
		*B = tmp;
	}
	else
	{
		int tmp;
		tmp = *B;
		*B = *A;
		*A = tmp;
	}
}
int main()
{
	int a = 36;
	int b = 26;
	swap(&a,&b);
	printf("valeur a: %d valeur b: %d \n", a, b);
	ordonne(&a, &b);
	printf("valeur a: %d valeur b: %d \n", a, b);
	system("pause");
	return(EXIT_SUCCESS);
}
